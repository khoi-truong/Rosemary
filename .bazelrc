build --apple_platform_type=ios
build --apple_generate_dsym
build --ios_simulator_device="iPhone 12"
build --ios_simulator_version=14.5
build --spawn_strategy=local
build --incompatible_use_python_toolchains=true
build --experimental_guard_against_concurrent_changes=true
build --collapse_duplicate_defines=true
build --incompatible_strict_action_env=true
build --incompatible_restrict_string_escapes=false
build --use_top_level_targets_for_symlinks

test --spawn_strategy=local
test --incompatible_use_python_toolchains=true
test --experimental_guard_against_concurrent_changes=true
test --collapse_duplicate_defines=true
test --incompatible_strict_action_env=true
test --incompatible_restrict_string_escapes=false
test --use_top_level_targets_for_symlinks
